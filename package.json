{
  "name": "express-starter-clean",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll --coverage --setupFiles dotenv/config",
    "lint": "eslint \"src/**/*.ts\" --fix",
